{
    "manifest_version": 2,
    "name": "FoxyQL",
    "version": "1.2",
    "description": "Firefox extension for the GraphiQL tool",
    "browser_action": {
        "default_title": "Launch GraphiQL",
        "default_icon": "public/logo.png"
    },
    "icons": {
        "16": "public/logo.png",
        "48": "public/logo.png",
        "128": "public/logo.png"
    },
    "permissions": [
        "http://*/",
        "https://*/",
        "storage"
    ]
}
